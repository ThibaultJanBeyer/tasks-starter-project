    "joi": "^17.5.0",
    "mongoose": "^6.1.7",
    "source-map-support": "^0.5.21",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.10",
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
