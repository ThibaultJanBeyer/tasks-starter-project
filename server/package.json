{
  "name": "tasks-starter-server",
  "version": "0.1.0",
  "description": "Awesome scrum planning poker game done right ",
  "main": "dist/app.js",
  "author": "Thibault Jan Beyer",
  "license": "None",
  "scripts": {
    "build": "tsc",
    "test": "yarn lint",
    "lint": "eslint --ext .tsx,ts src/",
    "start": "\"@Todo production ready startup in docker container\"",
    "setup:dev": "./bin/setup-dev.sh",
    "teardown:dev": "./bin/teardown-dev.sh",
    "prestart:dev": "yarn setup:dev",
    "start:dev": "concurrently \"tsc -w\" \"nodemon -r source-map-support/register dist/app.js\""
  },
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "joi": "^17.5.0",
    "mongoose": "^6.1.7",
    "source-map-support": "^0.5.21",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
